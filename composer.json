{
    "name": "fp/badaboom-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 wise error catcher",
    "keywords": ["Error", "Exception"],
    "homepage": "https://github.com/formapro/FpBadaBoomBundle",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Credit-Jeeves/BadaBoom.git"
        }
    ],
    "authors": [
	{
            "name": "Vadim Tyukov",
            "email": "brainreflex@gmail.com"
        },
        {
            "name": "Maksim Kotlyar",
            "email": "kotlyar.maksim@gmail.com"
        }        
    ],
    "require": {
        "php": ">=7.0",
        "fp/badaboom": "dev-master2",
        "symfony/debug": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/templating": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "fp/universal-error-catcher": "As exceptions and recoverable\fatal errors catcher."  
    },
    "autoload": {
        "psr-0": { "Fp\\BadaBoomBundle": "" }
    },
    "target-dir": "Fp/BadaBoomBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
